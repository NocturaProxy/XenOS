    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "moduleResolution": "node",
  },
  "include": ["public/xen/**/*.ts", "public/xen/**/*.tsx", "public/xen/**/*.js", "public/xen/**/*.jsx"],
}
